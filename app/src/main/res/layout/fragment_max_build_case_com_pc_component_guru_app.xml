<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".maximumBuild.MaxBuildCaseComPcComponentGuruApp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:src="@drawable/com_pc_component_guru_app_loading_image"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <RelativeLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/menuComPcComponentGuruApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="320dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/com_pc_component_guru_app_image_menu"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/starter_bui"
                android:layout_width="185.38dp"
                android:layout_height="44.31dp"
                android:layout_centerInParent="true"
                android:gravity="center_horizontal|top"
                android:lineSpacingExtra="8sp"
                android:text="@string/maximum_bui"
                android:textAppearance="@style/starter_bui"
                android:translationY="-4.01sp"
                app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
                app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
                app:layout_constraintStart_toStartOf="@+id/rectangle_1" />

            <ImageView
                android:id="@+id/backComPcComponentGuruApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerInParent="false"
                android:layout_marginStart="0dp"
                android:src="@drawable/image_back_com_pc_component_guru_app_loading_"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/rectangle_1"
                android:layout_width="202dp"
                android:layout_height="48dp"
                android:layout_centerInParent="true"
                android:layout_marginStart="115dp"
                android:background="@drawable/rectangle_1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/backComPcComponentGuruApp"
                app:layout_constraintTop_toTopOf="parent" />

        </RelativeLayout>

        <TextView
            android:id="@+id/ase"
            android:layout_width="61dp"
            android:layout_height="44dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="18dp"
            android:gravity="center_horizontal|top"
            android:lineSpacingExtra="10sp"
            android:text="@string/ase"
            android:textAppearance="@style/ase"
            android:textSize="27sp"
            android:translationY="-4.99sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" />


        <TextView
            android:id="@+id/eatx_phante"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="-15dp"
            android:gravity="center_horizontal|top"
            android:lineSpacingExtra="-5sp"
            android:text="@string/eatx_phante"
            android:textAppearance="@style/eatx_phante"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ase" />

        <TextView
            android:id="@+id/power_unit"
            android:layout_width="142dp"
            android:layout_height="44dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:gravity="center_horizontal|top"
            android:lineSpacingExtra="10sp"
            android:text="@string/power_unit"
            android:textAppearance="@style/power_unit"
            android:textSize="26sp"
            android:translationY="-4.99sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView51" />

        <TextView
            android:id="@+id/atx_seasoni"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="-10dp"
            android:gravity="center_horizontal|top"
            android:lineSpacingExtra="10sp"
            android:text="@string/atx_seasoni"
            android:textAppearance="@style/atx_seasoni"
            android:textSize="26sp"
            android:translationY="-4.99sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/power_unit" />

        <ImageView
            android:id="@+id/imageView51"
            android:layout_width="197dp"
            android:layout_height="189dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/mb_case_com_pc_component_guru_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/eatx_phante" />

        <ImageView
            android:id="@+id/imageView52"
            android:layout_width="230dp"
            android:layout_height="156dp"
            android:layout_marginTop="-10dp"
            android:src="@drawable/mb_pb_com_pc_component_guru_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/atx_seasoni" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>

